{
  "expo": {
    "name": "SoongsilBob",
    "slug": "SoongsilBob",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/horseIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.ACCESS_BACKGROUND_LOCATION"
      ],
      "package": "com.limsoft0401.SoongsilBob",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA8w7s_aPtbNIXRQ_fjM2Y-1zZ3fEiE4W0"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "숭실밥집이 위치권한을 요청합니다:)",
          "isAndroidBackgroundLocationEnabled": "숭실밥집이 위치권한을 요청합니다:)"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "168ce610-520e-4c98-b954-4236e6fc5a69"
      }
    }
  }
}
