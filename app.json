{
  "expo": {
    "name": "SoongsilBob",
    "slug": "SoongsilBob",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/horseIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "숭실밥집이 위치권한을 요청합니다:)",
          "isAndroidBackgroundLocationEnabled": "숭실밥집이 위치권한을 요청합니다:)"
        }
      ]
    ]
  }
}
